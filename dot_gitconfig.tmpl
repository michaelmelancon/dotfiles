# This is Git's per-user configuration file.
[user]
	name = Michael Melancon
	email = michaelmelancon@gmail.com
[fetch]
	prune = true
[init]
	defaultBranch = main
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[http]
	sslbackend = openssl
[credential]
	helper = manager
[credential "https://dev.azure.com"]
	usehttppath = true
[credential "https://gitlab.com"]
	provider = generic
[core]
	longpaths = true
[gpg]
	format = openpgp
[push]
	autoSetupRemote = true
[pull]
	rebase = false
[rebase]
	autoStash = true
	autosquash = false
	updateRefs = false
